{
  "name": "resume-builder-server",
  "version": "1.0.0",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "bun run --watch src/index.ts",
    "clean": "rm -rf dist && rm -f index.js",
    "build": "bun build ./src/index.ts --outdir dist --target bun",
    "build:prod": "bun install && bun run prisma generate && bun run build",
    "start:prod": "bun run dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "bun run prisma/seed.ts",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@react-pdf/renderer": "^4.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "slugify": "^1.6.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^24.8.1",
    "aws-sdk": "^2.1692.0",
    "bun-types": "latest",
    "mock-aws-s3": "^4.0.2",
    "nock": "^14.0.10",
    "prisma": "^6.17.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "module": "src/index.js"
}
